schema:Thing a sh:NodeShape ;
    ys:fromClass wd:Q35120 ;
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
        ys:fromProperty skos:altLabel ;
    ] ;
    sh:property [
        sh:path schema:description ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty schema:description ;
    ] ;
    sh:property [
        sh:path schema:image ;
        sh:node schema:ImageObject ;
        ys:fromProperty wdt:P18 ;
    ] ;
    sh:property [
        sh:path schema:name ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty skos:prefLabel ;
    ] ;
    sh:property [
        sh:path schema:sameAs ;
        sh:datatype xsd:anyURI ;
    ] ;
    sh:property [
        sh:path schema:url ;
        sh:datatype xsd:anyURI ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P856 ;
    ] ;
    sh:property [
        sh:path owl:sameAs ;
        sh:nodeKind sh:IRI ;
    ] .

